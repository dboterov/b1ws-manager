            <property name="hibernate.dialect" value="org.hibernate.dialect.SQLServerDialect"/>
            <property name="hibernate.show_sql" value="false"/>
            <property name="hibernate.format_sql" value="false"/>
        </properties>
    </persistence-unit>
    <persistence-unit name="IGBPruebasPU" transaction-type="JTA">
        <jta-data-source>java:/IGBPruebasDS</jta-data-source>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.SQLServerDialect"/>
            <property name="hibernate.show_sql" value="false"/>
            <property name="hibernate.format_sql" value="false"/>
        </properties>
    </persistence-unit>
    <persistence-unit name="VARROCPU" transaction-type="JTA">
        <jta-data-source>java:/VARROCDS</jta-data-source>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <properties>
            <property name="hibernate.dialect" value="org.hibernate.dialect.SQLServerDialect"/>
        </properties>
    </persistence-unit>
    <persistence-unit name="VARROCPruebasPU" transaction-type="JTA">
        <jta-data-source>java:/VARROCPruebasDS</jta-data-source>
